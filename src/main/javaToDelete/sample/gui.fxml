<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="javaToDelete.sample.Controller">
    <children>
        <GridPane layoutX="2.0" layoutY="-4.0" prefHeight="400.0" prefWidth="591.0">
            <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="140.0" minWidth="10.0" prefWidth="140.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="204.0" minWidth="10.0" prefWidth="165.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="169.0" minWidth="0.0" prefWidth="79.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="234.0" minWidth="10.0" prefWidth="163.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="236.0" minWidth="10.0" prefWidth="44.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints maxHeight="74.0" minHeight="10.0" prefHeight="74.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="85.0" minHeight="10.0" prefHeight="62.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="84.0" minHeight="10.0" prefHeight="84.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="70.0" minHeight="10.0" prefHeight="70.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="47.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="47.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="47.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="47.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            </rowConstraints>
            <children>
                <Label text="Get email" />
                <TextField fx:id="surnameEmailTextField" promptText="Type surname" GridPane.columnIndex="1" />
                <TextField fx:id="emailResultTextField" GridPane.columnIndex="3" />
                <Label text="Get salary" GridPane.rowIndex="1" />
                <TextField fx:id="salaryResultTextField" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                <TextField fx:id="surnameSalaryTextField" promptText="Type surname" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Button fx:id="emailButton" mnemonicParsing="false" onAction="#onButtonEmailButtonClicked" text="Process" GridPane.columnIndex="2" />
                <Button fx:id="salaryButton" mnemonicParsing="false" text="Process" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                <Label text="Update paygrade" GridPane.rowIndex="2" />
                <TextField fx:id="paygradeTextField" promptText="Type id" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <Button fx:id="paygradeButton" mnemonicParsing="false" text="Process" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                <Label text="Delete employee" GridPane.rowIndex="3" />
                <TextField fx:id="deleteTextField" promptText="Type id" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <Button fx:id="deleteButton" mnemonicParsing="false" text="Process" GridPane.columnIndex="2" GridPane.rowIndex="3" />
            <Label text="Add new employee" GridPane.rowIndex="4" />
            <TextField fx:id="addName" promptText="Name" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <TextField fx:id="addSurname" promptText="Surname" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <TextField fx:id="addEmail" promptText="Email" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <TextField fx:id="addPaygrade" promptText="Paygrade" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Button fx:id="addEmployeeButton" mnemonicParsing="false" onAction="#onButtonAddEmployeeClicked" text="Process" GridPane.columnIndex="2" GridPane.rowIndex="4" />
            </children>
        </GridPane>
    </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
